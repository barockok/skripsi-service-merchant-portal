<p id="notice"><%= notice %></p>

<h1>Merchants</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Website</th>
      <th>Industry</th>
      <th>Active</th>
      <th>Bank mid</th>
      <th>Notification url</th>
      <th>Notification email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @merchants.each do |merchant| %>
      <tr>
        <td><%= merchant.name %></td>
        <td><%= merchant.website %></td>
        <td><%= merchant.industry %></td>
        <td><%= merchant.active %></td>
        <td><%= merchant.bank_mid %></td>
        <td><%= merchant.notification_url %></td>
        <td><%= merchant.notification_email %></td>
        <td><%= link_to 'Show', merchant %></td>
        <td><%= link_to 'Edit', edit_merchant_path(merchant) %></td>
        <td><%= link_to 'Destroy', merchant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Merchant', new_merchant_path %>
